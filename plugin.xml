<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns:android="http://schemas.android.com/apk/res/android" id="com.customfileSbplugin" version="1">
  <name>customfileSbplugin</name>
  <engines>
    <engine name="cordova-android" version=">=8.1.0" />
  </engines>

   <asset src="www/hello.js" target="js/hello.js"/>

  <js-module src="www/hello.js" name="hello">
    <clobbers target="hello" />
  </js-module>


  <description>This plugin is used to manipulate the AndroidManifest.xml</description>
  <author>Shikha</author>
  <keywords>AndroidManifest</keywords>

  <license>MIT</license>

  <!-- android -->
  

   <platform name="android">
    <config-file target="res/xml/config.xml" parent="/*">
      <feature name="Hello">
        <param name="android-package" value="com.customfileSbplugin.Hello"/>
      </feature>
    </config-file>

    <source-file src="src/android/Hello.java" target-dir="src/com/customfileSbplugin/plugin/"/>
  </platform>
</plugin>
